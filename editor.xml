<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Lucida Sans Typewriter" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" pattern="@CEC" />
    <pattern icon="important" useCustomColors="true" pattern="@REMOVE_FILE">
      <option name="FOREGROUND" value="c00000" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern pattern="@BJS" />
    <pattern pattern="@RJW" />
    <pattern icon="important" useCustomColors="true" pattern="@REMOVE_FUNCTION">
      <option name="FOREGROUND" value="c00000" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="@replyto">
      <option name="FOREGROUND" value="c000a7" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="CEC">
      <pattern index="2" />
    </filter>
    <filter name="Remove Files">
      <pattern index="3" />
      <pattern index="6" />
    </filter>
    <filter name="DEV NAME">
      <pattern index="5" />
      <pattern index="4" />
      <pattern index="2" />
    </filter>
  </component>
</application>